{
    "name": "fadil/hfx",
    "description": "Basic Micro MVC",
    "authors": [
        {
            "name": "fadilxcoder",
            "email": "fadil@xcoder.dvlpr"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "symfony/http-kernel": "^5.2",
        "symfony/http-foundation": "^5.2",
        "vlucas/phpdotenv": "^5.3",
        "twig/twig": "^3.0",
        "php-di/php-di": "^6.3",
        "symfony/asset": "^5.2",
        "nikic/fast-route": "^1.3",
        "mnapoli/silly": "^1.7",
        "ghost-dvlpr/chrome-php": "dev-master",
        "ghost-dvlpr/memcache-gui": "dev-master",
        "fakerphp/faker": "^1.19",
        "robmorgan/phinx": "^0.12.12"
    },
    "require-dev": {
        "filp/whoops": "^2.13",
        "phpunit/phpunit":"^9",
        "phpunit/phpunit-selenium": "*",
        "php-webdriver/webdriver":"1.14.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/chrome"
        },
        {
            "type": "path",
            "url": "./packages/memcache"
        }
    ]    
}
