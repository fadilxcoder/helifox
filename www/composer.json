{
    "name": "fadil/helifox",
    "description": "PHP Micro Framework v5 - PHP 8.4",
    "authors": [
        {
            "name": "fadilxcoder",
            "email": "fadilxcoder@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "symfony/http-kernel": "^7.2",
        "symfony/http-foundation": "^7.2",
        "symfony/asset": "^7.2",
        "vlucas/phpdotenv": "^5.6",
        "twig/twig": "^3.14",
        "php-di/php-di": "^7.1",
        "nikic/fast-route": "^1.3",
        "mnapoli/silly": "^2.0",
        "fakerphp/faker": "^1.24",
        "robmorgan/phinx": "^0.16.0",
        "filp/whoops": "^2.16"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "squizlabs/php_codesniffer": "^3.9",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-di/php-di": true
        }
    }
}
